  <metadata>
    <id>Photon.MSBuild</id>
    <title>Photon MSBuild Plugin</title>
    <version>$version$</version>
    <authors>null511, Photon.CI</authors>
    <projectUrl>https://github.com/Photon-CI/Photon/tree/master/Plugins/Photon.MSBuild</projectUrl>
    <iconUrl>https://github.com/Photon-CI/Photon/raw/master/media/plugin_light.png</iconUrl>
    <description>A Photon plugin for invoking MSBuild.</description>
    <copyright>Copyright 2018</copyright>
    <tags>photon net automation MSBuild</tags>
    <dependencies>
      <dependency id="Photon.Framework" version="$frameworkVersion$" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\*.dll" exclude="**\Photon.Framework.dll" />
    <file src="bin\$configuration$\*.config" />
  </files>
</package>