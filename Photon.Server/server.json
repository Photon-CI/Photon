{
  "name": "Photon Server",
  "http": {
    "host": "localhost",
    "port": 8088,
    "path": "/photon/server"
  },
  "agents": [
    {
      "name": "Build Agent",
      "url": "http://localhost/photon/build",
      "tcpHost": "localhost",
      "tcpPort": 10933,
      "roles": [
        "build",
        "deploy.web",
        "deploy.svc"
      ]
    },{
      "name": "Deploy Agent",
      "url": "http://localhost/photon/deploy",
      "tcpHost": "localhost",
      "tcpPort": 10934,
      "roles": [
        "deploy"
      ]
    }
  ]
}
